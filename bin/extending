#!/usr/bin/env ruby

require_relative "../lib/kid"
require_relative "../lib/dancer"

puts Kid.metadata
puts Dancer.metadata

__END__
angelina = Dancer.new
angelina.twirl
# => "I'm twirling!"
angelina.jump
# => "Look how high I'm jumping!"

buster = Kid.new
buster.jump
# => "Look how high I'm jumping!"
buster.take_a_bow
# => "Thank you, thank you. It was a pleasure to dance for you all."
